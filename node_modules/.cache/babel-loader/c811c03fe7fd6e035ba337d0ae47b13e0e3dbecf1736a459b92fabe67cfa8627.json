{"ast":null,"code":"import { ref } from \"vue\";\nimport { Navigation } from \"swiper/modules\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/navigation\";\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  props: {\n    slides: Array\n  },\n  setup() {\n    const swiper = ref();\n    return {\n      modules: [Navigation],\n      swiper\n    };\n  }\n};","map":{"version":3,"names":["ref","Navigation","Swiper","SwiperSlide","components","props","slides","Array","setup","swiper","modules"],"sources":["C:\\gitPages\\rol-shop\\src\\components\\SliderOrder.vue"],"sourcesContent":["<template>\r\n  <div class=\"addition\">\r\n    <h5 class=\"addition-title\"></h5>\r\n    <div class=\"swiper-object-4\">\r\n      <swiper\r\n        ref=\"mySwiper\"\r\n        class=\"addition-slider\"\r\n        :modules=\"modules\"\r\n        :slides-per-view=\"4\"\r\n        :slides-per-group=\"3\"\r\n        :space-between=\"20\"\r\n      >\r\n        <swiper-slide v-for=\"item in slides\" :key=\"item.id\">\r\n          <div class=\"addition-slider__slide\">\r\n            <div class=\"addition-slider__imgwrapper\">\r\n              <img\r\n                class=\"addition-slider__img\"\r\n                :src=\"require(`@/img/products/${item.imgUrl}`)\"\r\n                alt=\"product\"\r\n              />\r\n            </div>\r\n            <h6 class=\"addition-slider__title\">{{ item.title }}</h6>\r\n            <p v-if=\"item.weight\" class=\"addition-slider__text\">{{ item.weight }} г</p>\r\n            <p v-else class=\"addition-slider__text\">{{ item.volume }} л</p>\r\n            <div class=\"addition-slider__box\">\r\n              <p class=\"addition-slider__price\">{{ item.price }}<span>грн</span></p>\r\n              <button class=\"addition-slider__btn\">\r\n                <img src=\"@/img/icon/plus.svg\" alt=\"\" />\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </swiper-slide>\r\n      </swiper>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\nimport { Navigation } from \"swiper/modules\";\r\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\r\nimport \"swiper/css\";\r\nimport \"swiper/css/navigation\";\r\nexport default {\r\n  components: {\r\n    Swiper,\r\n    SwiperSlide,\r\n  },\r\n  props: {\r\n    slides: Array,\r\n  },\r\n  setup() {\r\n    const swiper = ref();\r\n\r\n    return {\r\n      modules: [Navigation],\r\n      swiper,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.addition-slider {\r\n  &__price {\r\n    color: $grey-dark;\r\n    font-size: 24px;\r\n    font-weight: 700;\r\n    line-height: 80%;\r\n    span {\r\n      color: #686870;\r\n      font-size: 16px;\r\n      font-weight: 700;\r\n      margin-left: 5px;\r\n    }\r\n  }\r\n  &__slide {\r\n    width: 100%;\r\n    background-color: #fff;\r\n    border-radius: 24px;\r\n    height: 312px;\r\n    padding: 16px;\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  &__imgwrapper {\r\n    width: 100%;\r\n    height: 119px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    margin-bottom: 12px;\r\n  }\r\n\r\n  &__img {\r\n    object-fit: cover;\r\n  }\r\n\r\n  &__title {\r\n    color: $grey-dark;\r\n    font-weight: 700;\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  &__text {\r\n    color: rgb(255, 102, 51);\r\n    font-size: 14px;\r\n    font-weight: 500;\r\n  }\r\n\r\n  &__box {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin-top: auto;\r\n  }\r\n\r\n  &__btn {\r\n    width: 60px;\r\n    height: 38px;\r\n    back\r\n  }\r\n}\r\n.addition {\r\n}\r\n.addition-title {\r\n}\r\n.swiper-object-4 {\r\n}\r\n</style>\r\n"],"mappings":"AAsCA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,UAAS,QAAS,gBAAgB;AAC3C,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAC9B,eAAe;EACbC,UAAU,EAAE;IACVF,MAAM;IACNC;EACF,CAA