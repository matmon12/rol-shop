           <div class=\"success__data\">\r\n                    <p class=\"success__data-date\">\r\n                      {{ formattedDate[0] }} {{ order.year }}\r\n                    </p>\r\n                    <p class=\"success__data-number\"># {{ order.number }}</p>\r\n                  </div>\r\n                  <el-scrollbar class=\"success__product-scrollbar\">\r\n                    <ul class=\"success__product\">\r\n                      <li\r\n                        class=\"success__product-item\"\r\n                        v-for=\"(item, id) in order.items\"\r\n                        :key=\"id\"\r\n                      >\r\n                        <img\r\n                          class=\"success__product-img\"\r\n                          :src=\"require(`@/img/products/${item.imgUrl}`)\"\r\n                          alt=\"product\"\r\n                        />\r\n                      </li>\r\n                    </ul>\r\n                  </el-scrollbar>\r\n                  <div class=\"success__info\">\r\n                    <div class=\"success__info-item\">\r\n                      <p class=\"success__info-name\">Кол-во</p>\r\n                      <p class=\"success__info-value\">{{ count }}</p>\r\n                    </div>\r\n                    <div class=\"success__info-item\">\r\n                      <p class=\"success__info-name\">Итого</p>\r\n                      <p class=\"success__info-value\">\r\n                        {{ order.totalPrice }} <span>грн</span>\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"success__bottom-bottom\">\r\n                  <div class=\"success__steps\">\r\n                    <div\r\n                      class=\"success__steps-item\"\r\n                      v-for=\"(item, id) in order.methods === 'Доставка'\r\n                        ? stepsDelivery\r\n                        : stepsPickup\"\r\n                      :key=\"id\"\r\n                      :class=\"{ 'steps--check': id === 0 }\"\r\n                    >\r\n                      <div class=\"success__steps-top\">\r\n                        <div class=\"success__steps-lines\">\r\n                          <span class=\"line-left\"></span>\r\n                        </div>\r\n                        <div class=\"success__steps-number\">\r\n                          <span class=\"steps-number\">{{ id + 1 }}</span>\r\n                        </div>\r\n                        <div class=\"success__steps-lines\">\r\n                          <span class=\"line-right\"></span>\r\n                        </div>\r\n                      </div>\r\n                      <p class=\"success__steps-text\">\r\n                        {{ item.split(\" \")[0] }}<br />{{\r\n                          item.split(\" \").slice(1).join(\" \")\r\n                        }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n              <div class=\"success__products\">\r\n                <div\r\n                  class=\"success__products-item\"\r\n                  v-for=\"(item, id) in products\"\r\n                  :key=\"id\"\r\n                >\r\n                  <div class=\"success__products-imgwrapper\">\r\n                    <img\r\n                      class=\"success__products-img\"\r\n                      :src=\"require(`@/img/products/${item.imgUrl}`)\"\r\n                      alt=\"tovar\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"success__products-info\">\r\n                    <p class=\"success__products-title\">{{ item.title }}</p>\r\n                    <p v-if=\"item.weight\" class=\"success__products-weight\">\r\n                      Вес: {{ item.weight }} г\r\n                    </p>\r\n                    <p v-if=\"item.volume\" class=\"success__products-weight\">\r\n                      Объем: {{ item.volume }} л\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"success__products-box\">\r\n                    <div class=\"success__products-box-item\">\r\n                      <p class=\"success__products-box-name\">Цена</p>\r\n                      <p class=\"success__products-box-value\">\r\n                        {{ item.price }} <span>грн</span>\r\n                      </p>\r\n                    </div>\r\n                    <div class=\"success__products-box-item\">\r\n                      <p class=\"success__products-box-name\">Кол-во</p>\r\n                      <p class=\"success__products-box-value\">{{ item.count }}</p>\r\n                    </div>\r\n                    <div class=\"success__products-box-item\">\r\n                      <p class=\"success__products-box-name\">Итого</p>\r\n                      <p class=\"success__products-box-value\">\r\n                        {{ item.price * item.count }} <span>грн</span>\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </el-collapse-item>\r\n          </el-collapse>\r\n        </div>\r\n        <div class=\"success__btn-wrapper\">\r\n          <button v-if=\"order.methods === 'Самовывоз'\" @click=\"dialogVisible = true\" class=\"success__btn-direction\">Проложить маршрут</button>\r\n          <button class=\"success__btn\" @click=\"$router.push('/rol-shop/')\">\r\n            Вернутся на главную\r\n          </button>\r\n        </div>\r\n        <el-dialog v-model=\"dialogVisible\" title=\"Построение маршрута\" align-center>\r\n          <p class=\"success__dialog-text\">Здесь вы можете просмотреть маршрут в точку выдачи</p>\r\n          <div class=\"success__map\">\r\n            <Map :geocoderCoord=\"geocoderCoord\" :newMarker=\"newMarker\" ref=\"Map\" />\r\n          </div>\r\n        </el-dialog>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { inject } from \"vue\";\r\nimport { mapState, mapActions } from \"vuex\";\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            order: [],\r\n            count: 0,\r\n            formattedDate: \"\",\r\n            formattedOptions: \"\",\r\n            isVisible: false,\r\n            products: [],\r\n            dialogVisible: false,\r\n            newMarker: [],\r\n            stepsDelivery: [\r\n                \"Подтверждение заказа\",\r\n                \"Приготовление\",\r\n                \"Ожидание курьера\",\r\n                \"Курьер в пути\",\r\n                \"Курьер на месте\",\r\n            ],\r\n            stepsPickup: [\r\n                \"Подтверждение заказа\",\r\n                \"Приготовление\",\r\n                \"Отправка в пункт выдачи\",\r\n                \"Ожидает в пункте выдачи\",\r\n                \"Заказ получен\",\r\n            ],\r\n        };\r\n    },\r\n    methods: {\r\n        ...mapActions({\r\n            geocoderAddress: \"map/geocoderAddress\",\r\n        }),\r\n        async fetchOrder() {\r\n            try {\r\n                const { data } = await axios.get(`https://c2ff7a8de0a119c8.mokky.dev/orders?number=${this.historyOrder}`);\r\n                this.order = data[0];\r\n            }\r\n            catch (err) {\r\n                console.log(err);\r\n            }\r\n        },\r\n        dateFormat(dayDate, yearDate, timeDate) {\r\n            const [day, month] = dayDate.split(\".\");\r\n            const [hours, minutes] = timeDate.split(\":\");\r\n            const date = new Date(yearDate, month - 1, day, hours, minutes);\r\n            const formattedTime = date.toLocaleTimeString(\"ru-RU\", {\r\n                hour: \"numeric\",\r\n                minute: \"2-digit\",\r\n            });\r\n            const months = [\r\n                \"января\",\r\n              